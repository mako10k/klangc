# COMMENT
\~args -> ~println "hoge\n";
~join = \~m -> ~chain ~m ~id;
~id = \~x -> ~x;
~fix = \~f -> { ~x ; ~x = ~f ~x; };
~chain = \~m -> \~f -> ~join (~fmap ~f ~m);
~fmap = \~f -> \~m -> ~chain ~m \~x -> ~return (~f ~x);

~pred = \(Cons ~x ~y) -> ~y;

~fib = ~fix \~f -> {
  \0 -> 1;
  \1 -> 1;
  \Int ~n -> {~add (~f ~n_1) (~f ~n_2);
  ~n_1 = ~pred ~n;
  ~n_2 = ~pred ~n_1;};
};
~f = \~x -> \~x -> ~x;